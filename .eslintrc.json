{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        // tabbed indentation
        "indent": [2, "tab"],
        "no-tabs": 0,
        "space-in-parens": ["error", "always"]
    }
}
